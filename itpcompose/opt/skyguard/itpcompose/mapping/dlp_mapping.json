{
    "template": "ep_dlp-*",
    "order":1,
    "settings": {
        "index.number_of_shards" : 1,
        "index.number_of_replicas" : 0,
        "index.refresh_interval" : "60s",
        "analysis": {
            "char_filter": {
                "quote": {
                    "type": "mapping",
                    "mappings": [
                        "« => \"",
                        "» => \""
                    ]
                }
            },
            "normalizer": {
                "lowercase_normalizer": {
                    "type": "custom",
                    "char_filter": ["quote"],
                    "filter": ["lowercase", "asciifolding"]
                }
            }
        }
    },
